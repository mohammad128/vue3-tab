{
  "name": "vue3-tab",
  "version": "1.0.34",
  "description": "Vue 3 Tab",
  "main": "dist/vue3-tab.ssr.js",
  "browser": "dist/vue3-tab.esm.js",
  "module": "dist/vue3-tab.esm.js",
  "unpkg": "dist/vue3-tab.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "sideEffects": false,
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.js",
    "prebuild": "rimraf ./dist",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "dependencies": {
    "rollup-plugin-vue-global-styles": "^0.1.0",
    "v-wave": "^1.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "@vue/compiler-sfc": "^3.0.11",
    "cross-env": "^7.0.3",
    "minimist": "^1.2.5",
    "postcss": "^8.2.10",
    "rimraf": "^3.0.2",
    "rollup": "^2.52.8",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "vue": "^3.0.5"
  },
  "peerDependencies": {
    "vue": "^3.0.5"
  },
  "engines": {
    "node": ">=12"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mohammad128/vue3-tab.git"
  },
  "keywords": [
    "Javascript",
    "Vue",
    "Vue3",
    "Tab",
    "Vue Tab",
    "Vue3 Tab",
    "Tab Switcher",
    "Tab3 Switcher",
    "Telegram Tab",
    "Vue Telegram Tab",
    "Vue3 Telegram Tab",
    "Vue Tab Switcher"
  ],
  "author": "MohammadJavadi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mohammad128/vue3-tab/issues"
  },
  "homepage": "https://github.com/mohammad128/vue3-tab#readme"
}
